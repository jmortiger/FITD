{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/Fitd/Fitd",//"enter program name, for example ${workspaceFolder}/a.out",
			// "args": ["-pak", "511"],
			// "args": ["-pak", "-floor", "-mask", "-camera", "-sound"],
			// "args": ["-pak", "511", "-floor", "511", "-mask", "511", "-camera", "511", "-sound", "511", "-itd", "511"],
			// "args": ["-floor", "511", "-sound", "511"],
			// "args": ["-pak", "-floor", "-mask", "-camera", "-sound", "-logLife", "549,11"],
			"args": ["-pak", "-floor", "-mask", "-camera", "-sound", "-logLife", "16,17,18"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/data", //"${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}